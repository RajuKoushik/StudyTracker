<?xml version="1.0" encoding="utf-8"?><!-- suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyapp.studytracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudyModeActivity"
            android:label="@string/title_activity_study_mode"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddSubject"
            android:label="@string/title_activity_add_subject"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SubjectListActivity"
            android:label="@string/title_activity_subject_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>